    </estimate>
    <write>
      <cleanup>
        1
      </cleanup>
      <wrt_cor>
        1
      </wrt_cor>
    </write>
  </segment>
  <bias>
    <nbins>
      256
    </nbins>
    <reg>
      0.01
    </reg>
    <cutoff>
      30
    </cutoff>
  </bias>
</xdefaults>
